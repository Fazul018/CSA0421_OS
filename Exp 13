#include <stdio.h>

int main() {
    int n, capacity;
    printf("Enter number of pages: ");
    scanf("%d", &n);
    int pages[n];
    printf("Enter page reference string: ");
    for(int i=0;i<n;i++)
        scanf("%d",&pages[i]);

    printf("Enter number of frames: ");
    scanf("%d",&capacity);

    int frame[capacity];
    int time[capacity]; // To store last used time
    for(int i=0;i<capacity;i++) frame[i]=-1;

    int count=0, t=0;

    printf("\nPage\tFrames\tPage Fault\n");
    for(int i=0;i<n;i++){
        int flag=0;
        // Check if page already in frame
        for(int j=0;j<capacity;j++){
            if(frame[j]==pages[i]){
                flag=1;
                time[j]=t++; 
                break;
            }
        }

        if(flag==0){ // Page fault
            int lru_index=0, min=time[0];
            // Find least recently used
            for(int j=0;j<capacity;j++){
                if(frame[j]==-1){ lru_index=j; break; } 
                if(time[j]<min){ min=time[j]; lru_index=j; }
            }

            frame[lru_index]=pages[i];
            time[lru_index]=t++;
            count++;

            printf("%d\t", pages[i]);
            for(int j=0;j<capacity;j++) if(frame[j]!=-1) printf("%d ",frame[j]);
            printf("\tYes\n");
        }
        else{ 
            printf("%d\t", pages[i]);
            for(int j=0;j<capacity;j++) if(frame[j]!=-1) printf("%d ",frame[j]);
            printf("\tNo\n");
        }
    }

    printf("\nTotal Page Faults: %d\n", count);
    return 0;
}


input :
Enter number of pages: 12
Enter page reference string: 1 2 3 4 1 2 5 1 2 3 4 5
Enter number of frames: 3


output :
Page    Frames      Page Fault
1       1           Yes
2       1 2         Yes
3       1 2 3       Yes
4       4 2 3       Yes
1       1 4 3       Yes
2       2 1 3       Yes
5       5 2 3       Yes
1       1 5 3       Yes
2       2 1 3       Yes
3       3 2 1       Yes
4       4 3 1       Yes
5       5 4 1       Yes

Total Page Faults: 12

