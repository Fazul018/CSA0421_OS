#include <stdio.h>
#include <string.h>

int main() {
    char records[20][50];
    int n, i, pos;

    printf("Enter number of records: ");
    scanf("%d", &n);

    printf("Enter %d records:\n", n);
    for (i = 0; i < n; i++)
        scanf("%s", records[i]);

    printf("Enter record position to access (1 to %d): ", n);
    scanf("%d", &pos);

    printf("\nReading records sequentially:\n");
    for (i = 0; i < pos; i++)
        printf("Record %d: %s\n", i + 1, records[i]);

    printf("\nAccessed Record = %s\n", records[pos - 1]);

    return 0;
}



input :
Enter number of records: 5
Enter 5 records:
A1
B2
C3
D4
E5
Enter record position to access (1 to 5): 4


output :
Reading records sequentially:
Record 1: A1
Record 2: B2
Record 3: C3
Record 4: D4

Accessed Record = D4
